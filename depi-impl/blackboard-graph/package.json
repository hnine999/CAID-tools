        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "^5.0.1",
        "reactflow": "^11.5.6",
        "use-debounce": "^9.0.4",
        "web-worker": "^1.2.0"
