    "version": "0.0.1",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build && node post_build.js",
        "test": "jest --config jest.config.js",
        "eject": "react-scripts eject",
        "lint": "eslint --ext .js,.jsx ./src",
        "lint:fix": "eslint --fix --ext .js,.jsx ./src"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "babel": {
        "presets": [
            "@babel/preset-react",
